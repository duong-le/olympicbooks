{
  "name": "store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/store/server/main.js",
    "start:dev": "ng serve --open --host 0.0.0.0 --disable-host-check",
    "build": "ng build --configuration production --outputHashing=all && ng run store:server:production && touch dist/store/browser/robots.txt",
    "start:dev:ssr": "ng run store:serve-ssr",
    "prerender": "ng run store:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.6",
    "@angular/common": "^14.2.6",
    "@angular/compiler": "^14.2.6",
    "@angular/core": "^14.2.6",
    "@angular/forms": "^14.2.6",
    "@angular/platform-browser": "^14.2.6",
    "@angular/platform-browser-dynamic": "^14.2.6",
    "@angular/platform-server": "^14.2.6",
    "@angular/router": "^14.2.6",
    "@nguniversal/express-engine": "^14.2.0",
    "device-detector-js": "^3.0.3",
    "express": "^4.18.2",
    "ng-zorro-antd": "^14.1.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.6",
    "@angular/language-service": "^14.2.6",
    "@nguniversal/builders": "^14.2.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.4",
    "typescript": "4.6.4"
  }
}
