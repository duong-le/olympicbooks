{
  "name": "store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/store/server/main.js",
    "start:dev": "ng serve --open --host 0.0.0.0 --disable-host-check",
    "build": "ng build --configuration production --outputHashing=all && ng run store:server:production && touch dist/store/browser/robots.txt",
    "start:dev:ssr": "ng run store:serve-ssr",
    "prerender": "ng run store:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.3",
    "@angular/common": "~12.2.3",
    "@angular/compiler": "~12.2.3",
    "@angular/core": "~12.2.3",
    "@angular/forms": "~12.2.3",
    "@angular/platform-browser": "~12.2.3",
    "@angular/platform-browser-dynamic": "~12.2.3",
    "@angular/platform-server": "~12.2.3",
    "@angular/router": "~12.2.3",
    "@nguniversal/express-engine": "^12.1.0",
    "device-detector-js": "^2.2.10",
    "express": "^4.17.1",
    "ng-zorro-antd": "^12.0.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.3",
    "@angular/language-service": "~12.2.3",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.13",
    "@types/node": "^14.17.12",
    "typescript": "4.3.5"
  }
}
