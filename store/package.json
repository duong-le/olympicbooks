{
  "name": "store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/store/server/main.js",
    "start:dev": "ng serve --open --host 0.0.0.0 --disable-host-check",
    "build": "ng build --prod --outputHashing=all && ng run store:server:production && touch dist/store/browser/robots.txt ",
    "start:dev:ssr": "ng run store:serve-ssr",
    "prerender": "ng run store:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/platform-server": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@nestjsx/crud-request": "^4.6.2",
    "@nguniversal/express-engine": "^9.1.1",
    "express": "^4.17.1",
    "ng-zorro-antd": "^9.3.0",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@nguniversal/builders": "^9.1.1",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.12",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  }
}
