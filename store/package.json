{
  "name": "store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/store/server/main.js",
    "start:dev": "ng serve --open --host 0.0.0.0 --disable-host-check",
    "build": "ng build --prod --outputHashing=all && ng run store:server:production && touch dist/store/browser/robots.txt ",
    "start:dev:ssr": "ng run store:serve-ssr",
    "prerender": "ng run store:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.3",
    "@angular/common": "~10.2.3",
    "@angular/compiler": "~10.2.3",
    "@angular/core": "~10.2.3",
    "@angular/forms": "~10.2.3",
    "@angular/platform-browser": "~10.2.3",
    "@angular/platform-browser-dynamic": "~10.2.3",
    "@angular/platform-server": "~10.2.3",
    "@angular/router": "~10.2.3",
    "@nestjsx/crud-request": "^4.6.2",
    "@nguniversal/express-engine": "^10.1.0",
    "device-detector-js": "^2.2.5",
    "express": "^4.17.1",
    "ng-zorro-antd": "^10.1.2",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.3",
    "@angular/language-service": "~10.2.3",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.9",
    "@types/node": "^12.19.7",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
